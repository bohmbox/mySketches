class Shape {
  // class properties
  int x;
  int y;
  int w;
  int h;

  // constructors
  Shape(){}

  Shape(int x,int  y,int  w,int h){
    this.x = x;
    this.y = y;
    this.w = w;
    this.h = h;
  }

  void setX(int x){
    this.x = x;
  }

  void setY(int y){
    this.y = y;
  }

  void setW(int w){
    this.w = w;
  }

  void setH(int h){
    this.h = h;
  }

}

